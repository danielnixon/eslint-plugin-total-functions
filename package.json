{
  "name": "eslint-plugin-total-functions",
  "version": "7.0.7",
  "description": "An ESLint plugin to enforce the use of TypeScript total functions.",
  "main": "dist",
  "repository": "https://github.com/danielnixon/eslint-plugin-total-functions.git",
  "author": "Daniel Nixon <dan.nixon@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@stryker-mutator/core": "6.4.2",
    "@stryker-mutator/jest-runner": "6.4.2",
    "@stryker-mutator/typescript-checker": "6.4.2",
    "@types/eslint": "8.37.0",
    "@types/jest": "29.5.0",
    "codecov": "3.8.3",
    "eslint": "8.37.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-config-typed-fp": "5.1.0",
    "eslint-plugin-functional": "5.0.7",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-sonarjs": "0.19.0",
    "eslint-plugin-total-functions": "7.0.6",
    "jest": "29.5.0",
    "patch-package": "6.5.1",
    "prettier": "2.8.7",
    "ts-jest": "29.1.0",
    "type-coverage": "2.25.0",
    "typescript": "5.0.3"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@typescript-eslint/type-utils": "^5.56.0",
    "@typescript-eslint/utils": "^5.56.0",
    "is-immutable-type": "^1.2.9",
    "tsutils": "^3.21.0"
  },
  "peerDependencies": {
    "eslint": "^8.36.0",
    "typescript": "^5.0.2"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint . --ext .js,.ts,.tsx --report-unused-disable-directives",
    "format": "prettier --write 'src/**/*.{ts,tsx}'",
    "test": "jest",
    "release": "yarn build && yarn type-coverage && yarn publish",
    "mutation-test": "stryker run"
  },
  "typeCoverage": {
    "atLeast": 99.96,
    "strict": true,
    "detail": true
  }
}
